<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Farbdefinitionen -->
    <Color x:Key="VibrantPink">#E05263</Color>
    <Color x:Key="FreshGreen">#53E064</Color>
    <Color x:Key="DeepSeaBlue">#284B63</Color>
    <Color x:Key="DarkSlate">#102533</Color>
    <Color x:Key="DunklesMarineblau">#090B32</Color>
    <Color x:Key="LightGrayish">#E2E5E6</Color>
    <Color x:Key="GrayishBlue">#919A9C</Color>

    <!-- Hintergrundfarben -->
    <SolidColorBrush x:Key="BackgroundVibrantPink" Color="#E05263"/>
    <SolidColorBrush x:Key="BackgroundFreshGreen" Color="#53E064"/>
    <SolidColorBrush x:Key="BackgroundDeepSeaBlue" Color="#284B63"/>
    <SolidColorBrush x:Key="BackgroundDarkSlate" Color="#102533"/>
    <SolidColorBrush x:Key="BackgroundDunklesMarineblau" Color="#090B32"/>
    <SolidColorBrush x:Key="BackgroundLightGrayish" Color="#E2E5E6"/>
    <SolidColorBrush x:Key="BackgroundGrayishBlue" Color="#919A9C"/>

    <!-- Textfarben -->
    <SolidColorBrush x:Key="TextVibrantPink" Color="#E05263"/>
    <SolidColorBrush x:Key="TextFreshGreen" Color="#53E064"/>
    <SolidColorBrush x:Key="TextDeepSeaBlue" Color="#284B63"/>
    <SolidColorBrush x:Key="TextDarkSlate" Color="#102533"/>
    <SolidColorBrush x:Key="TextLightGrayish" Color="#E2E5E6"/>
    <SolidColorBrush x:Key="TextGrayishBlue" Color="#919A9C"/>

    <!-- Botton Farbe -->



</ResourceDictionary>